{
    "buildCommand": "prisma generate --no-engine && next build && storybook build -o public/storybook --quiet",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/storybook",
            "destination": "/storybook-static/index.html"
        },
        {
            "source": "/storybook/(.*)",
            "destination": "/storybook-static/$1"
        }
    ],
    "headers": [
        {
            "source": "/storybook-static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}
