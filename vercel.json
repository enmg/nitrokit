{
    "buildCommand": "prisma generate --no-engine && next build && BASE_PATH=/storybook/ storybook build -o storybook-staticclea",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/storybook/(.*)",
            "destination": "/storybook-static/$1"
        }
    ],
    "headers": [
        {
            "source": "/storybook/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}
